{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.1",
      "toolsId": "3e21e261-ea1d-4ab9-b414-88432134115b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/northwind/northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true,
        "viewPath": "project1.view"
      },
      "routes": [
        {
          "name": "Categories",
          "pattern": "",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "Products",
          "pattern": "view2",
          "target": [
            "TargetView2"
          ]
        },
        {
          "name": "Customers",
          "pattern": "view3",
          "target": [
            "TargetView3"
          ]
        },
        {
          "name": "Orders",
          "pattern": "view4",
          "target": [
            "TargetView4"
          ]
        },
        {
          "name": "Employees",
          "pattern": "view5",
          "target": [
            "TargetView5"
          ]
        },
        {
          "name": "Suppliers",
          "pattern": "view6",
          "target": [
            "TargetView6"
          ]
        },
        {
          "name": "Shippers",
          "pattern": "view7",
          "target": [
            "TargetView7"
          ]
        },
        {
          "name": "Regions",
          "pattern": "view8",
          "target": [
            "TargetView8"
          ]
        },
        {
          "name": "Invoices",
          "pattern": "view9",
          "target": [
            "TargetView9"
          ]
        },
        {
          "name": "Sales",
          "pattern": "view10",
          "target": [
            "TargetView10"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "CategoriesView1"
        },
        "TargetView2": {
          "id": "View2",
          "name": "ProductsView2"
        },
        "TargetView3": {
          "id": "View3",
          "name": "CustomersView3"
        },
        "TargetView4": {  
          "id": "View4",
          "name": "OrdersView4"
        },
        "TargetView5": {  
          "id": "View5",
          "name": "EmployeesView5"
        },
        "TargetView6": {  
          "id": "View6",
          "name": "SuppliersView6"
        },
        "TargetView7": {  
          "id": "View7",
          "name": "ShippersView7"
        },
        "TargetView8": {  
          "id": "View8",
          "name": "RegionsView8"
        },
        "TargetView9": {  
          "id": "View9",
          "name": "InvoicesView9"
        },
        "TargetView10": {  
          "id": "View10",
          "name": "SalesView10"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
